# klavyeden -1 girilene kadar sayıların toplamı
toplam=0

while (True):
    sayi=int(input("sayı giriniz"))
    
        
    if sayi==-1:
        print("sayi toplam ",toplam)

        break
    else:
        toplam+=sayi
    
        continue




